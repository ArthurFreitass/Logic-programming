programa
{
    funcao inicio()
    {
        inteiro n, i, soma = 0
        
        escreva("Digite o valor de N: ")
        leia(n)
        
        para (i = 1; i <= n; i++) {
            soma = soma + i
        }
        
        escreva("A soma dos primeiros ", n, " números naturais é: ", soma, "\n")
    }
}
